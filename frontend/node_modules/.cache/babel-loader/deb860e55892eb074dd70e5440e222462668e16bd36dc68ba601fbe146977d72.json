{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      title: '',\n      ingredients: '',\n      instructions: '',\n      image: null\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      this.image = event.target.files[0];\n    },\n    addRecipe() {\n      let formData = new FormData();\n      formData.append('title', this.title);\n      formData.append('ingredients', this.ingredients);\n      formData.append('instructions', this.instructions);\n      if (this.image) {\n        formData.append('image', this.image);\n      }\n      axios.post('http://localhost:8000/api/recipes/', formData, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        console.log('Recipe added successfully:', response);\n        this.$router.push({\n          name: 'RecipeList'\n        }); // Navigate to RecipeList view\n      }).catch(error => {\n        console.error('Error adding recipe:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","title","ingredients","instructions","image","methods","handleFileUpload","event","target","files","addRecipe","formData","FormData","append","post","headers","then","response","console","log","$router","push","name","catch","error"],"sources":["C:\\Users\\mantegbosh.assefa\\Desktop\\django\\frontend\\src\\components\\AddRecipe.vue"],"sourcesContent":["<template>\r\n  <div class=\"edit-recipe\">\r\n    <h2 class=\"edit-recipe__title\">Add Recipe</h2>\r\n    <form class=\"edit-recipe__form\" @submit.prevent=\"addRecipe\">\r\n      <div class=\"edit-recipe__form-group\">\r\n        <label class=\"edit-recipe__label\" for=\"title\">Title</label>\r\n        <input class=\"edit-recipe__input\" type=\"text\" id=\"title\" v-model=\"title\" required>\r\n      </div>\r\n      <div class=\"edit-recipe__form-group\">\r\n        <label class=\"edit-recipe__label\" for=\"ingredients\">Ingredients</label>\r\n        <textarea class=\"edit-recipe__textarea\" id=\"ingredients\" v-model=\"ingredients\" rows=\"6\" required></textarea>\r\n      </div>\r\n      <div class=\"edit-recipe__form-group\">\r\n        <label class=\"edit-recipe__label\" for=\"instructions\">Instructions</label>\r\n        <textarea class=\"edit-recipe__textarea\" id=\"instructions\" v-model=\"instructions\" rows=\"10\" required></textarea>\r\n      </div>\r\n      <div class=\"edit-recipe__form-group\">\r\n        <label class=\"edit-recipe__label\" for=\"image\">Image</label>\r\n        <input class=\"edit-recipe__input-file\" type=\"file\" id=\"image\" @change=\"handleFileUpload\" accept=\"image/*\">\r\n      </div>\r\n      <button class=\"edit-recipe__submit-btn\" type=\"submit\">Add Recipe</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      title: '',\r\n      ingredients: '',\r\n      instructions: '',\r\n      image: null\r\n    };\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      this.image = event.target.files[0];\r\n    },\r\n    addRecipe() {\r\n      let formData = new FormData();\r\n      formData.append('title', this.title);\r\n      formData.append('ingredients', this.ingredients);\r\n      formData.append('instructions', this.instructions);\r\n      if (this.image) {\r\n        formData.append('image', this.image);\r\n      }\r\n\r\n      axios.post('http://localhost:8000/api/recipes/', formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Recipe added successfully:', response);\r\n        this.$router.push({ name: 'RecipeList' }); // Navigate to RecipeList view\r\n      })\r\n      .catch(error => {\r\n        console.error('Error adding recipe:', error);\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.edit-recipe {\r\n  max-width: 400px;\r\n  margin: 2rem auto;\r\n  padding: 2rem;\r\n  background-color: #f8f8f8;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.edit-recipe__title {\r\n  text-align: center;\r\n  margin-bottom: 1.5rem;\r\n  color: #333;\r\n}\r\n\r\n.edit-recipe__form-group {\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.edit-recipe__label {\r\n  display: block;\r\n  font-weight: bold;\r\n  margin-bottom: 0.5rem;\r\n  color: #555;\r\n}\r\n\r\n.edit-recipe__input,\r\n.edit-recipe__textarea {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  color: #333;\r\n}\r\n\r\n.edit-recipe__input-file {\r\n  display: block;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.edit-recipe__submit-btn {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 0.75rem 1.5rem;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.edit-recipe__submit-btn:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.edit-recipe__submit-btn:active {\r\n  background-color: #00408d;\r\n}\r\n</style>\r\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACH,KAAI,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACpC,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAIC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC7BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACZ,KAAK,CAAC;MACpCU,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAACX,WAAW,CAAC;MAChDS,QAAQ,CAACE,MAAM,CAAC,cAAc,EAAE,IAAI,CAACV,YAAY,CAAC;MAClD,IAAI,IAAI,CAACC,KAAK,EAAE;QACdO,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACT,KAAK,CAAC;MACtC;MAEAL,KAAK,CAACe,IAAI,CAAC,oCAAoC,EAAEH,QAAQ,EAAE;QACzDI,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAA