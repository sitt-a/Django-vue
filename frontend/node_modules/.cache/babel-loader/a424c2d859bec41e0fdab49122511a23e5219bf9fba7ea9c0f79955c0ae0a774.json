{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '../apiService';\nexport default {\n  data() {\n    return {\n      id: null,\n      title: '',\n      ingredients: '',\n      instructions: '',\n      image: null\n    };\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.fetchRecipe();\n  },\n  methods: {\n    fetchRecipe() {\n      api.get(`recipes/${this.id}/`).then(response => {\n        const recipe = response.data;\n        this.title = recipe.title;\n        this.ingredients = recipe.ingredients;\n        this.instructions = recipe.instructions;\n      }).catch(error => {\n        console.error('Error fetching recipe:', error);\n      });\n    },\n    onFileChange(e) {\n      this.image = e.target.files[0];\n    },\n    updateRecipe() {\n      let formData = new FormData();\n      formData.append('title', this.title);\n      formData.append('ingredients', this.ingredients);\n      formData.append('instructions', this.instructions);\n      if (this.image) {\n        formData.append('image', this.image);\n      }\n      api.put(`recipes/${this.id}/`, formData, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        console.log('Recipe updated:', response.data);\n        this.$router.push('/');\n      }).catch(error => {\n        console.error('Error updating recipe:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["api","data","id","title","ingredients","instructions","image","mounted","$route","params","fetchRecipe","methods","get","then","response","recipe","catch","error","console","onFileChange","e","target","files","updateRecipe","formData","FormData","append","put","headers","log","$router","push"],"sources":["C:\\Users\\mantegbosh.assefa\\Desktop\\django\\frontend\\src\\components\\EditRecipe.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Edit Recipe</h1>\r\n    <form @submit.prevent=\"updateRecipe\">\r\n      <div>\r\n        <label for=\"title\">Title:</label>\r\n        <input type=\"text\" v-model=\"title\" />\r\n      </div>\r\n      <div>\r\n        <label for=\"ingredients\">Ingredients:</label>\r\n        <textarea v-model=\"ingredients\"></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"instructions\">Instructions:</label>\r\n        <textarea v-model=\"instructions\"></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"image\">Image:</label>\r\n        <input type=\"file\" @change=\"onFileChange\" />\r\n      </div>\r\n      <button type=\"submit\">Update Recipe</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from '../apiService';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: null,\r\n      title: '',\r\n      ingredients: '',\r\n      instructions: '',\r\n      image: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.id = this.$route.params.id;\r\n    this.fetchRecipe();\r\n  },\r\n  methods: {\r\n    fetchRecipe() {\r\n      api.get(`recipes/${this.id}/`)\r\n        .then(response => {\r\n          const recipe = response.data;\r\n          this.title = recipe.title;\r\n          this.ingredients = recipe.ingredients;\r\n          this.instructions = recipe.instructions;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching recipe:', error);\r\n        });\r\n    },\r\n    onFileChange(e) {\r\n      this.image = e.target.files[0];\r\n    },\r\n    updateRecipe() {\r\n      let formData = new FormData();\r\n      formData.append('title', this.title);\r\n      formData.append('ingredients', this.ingredients);\r\n      formData.append('instructions', this.instructions);\r\n      if (this.image) {\r\n        formData.append('image', this.image);\r\n      }\r\n\r\n      api.put(`recipes/${this.id}/`, formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      })\r\n      .then(response => {\r\n        console.log('Recipe updated:', response.data);\r\n        this.$router.push('/');\r\n      })\r\n      .catch(error => {\r\n        console.error('Error updating recipe:', error);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Add similar styling as in the AddRecipe.vue */\r\nform div {\r\n  margin-bottom: 1em;\r\n}\r\n\r\nform label {\r\n  display: block;\r\n  margin-bottom: 0.5em;\r\n  font-weight: bold;\r\n}\r\n\r\nform input[type=\"text\"],\r\nform textarea {\r\n  width: 100%;\r\n  padding: 0.5em;\r\n  margin-bottom: 0.5em;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\nform input[type=\"file\"] {\r\n  margin-top: 1em;\r\n}\r\n\r\nbutton {\r\n  display: inline-block;\r\n  padding: 0.75em 1.5em;\r\n  background-color: #007BFF;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n  margin-top: 1em;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\nbutton:active {\r\n  background-color: #00408d;\r\n}\r\n</style>\r\n"],"mappings":";AA0BA,OAAOA,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,EAAE,EAAE,IAAI;MACRC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACL,EAAC,GAAI,IAAI,CAACM,MAAM,CAACC,MAAM,CAACP,EAAE;IAC/B,IAAI,CAACQ,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZV,GAAG,CAACY,GAAG,CAAC,WAAW,IAAI,CAACV,EAAE,GAAG,EAC1BW,IAAI,CAACC,QAAO,IAAK;QAChB,MAAMC,MAAK,GAAID,QAAQ,CAACb,IAAI;QAC5B,IAAI,CAACE,KAAI,GAAIY,MAAM,CAACZ,KAAK;QACzB,IAAI,CAACC,WAAU,GAAIW,MAAM,CAACX,WAAW;QACrC,IAAI,CAACC,YAAW,GAAIU,MAAM,CAACV,YAAY;MACzC,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD,CAAC,CAAC;IACN,CAAC;IACDE,YAAYA,CAACC,CAAC,EAAE;MACd,IAAI,CAACd,KAAI,GAAIc,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAChC,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,IAAIC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC7BD,QAAQ,CAACE,MAAM,CAA