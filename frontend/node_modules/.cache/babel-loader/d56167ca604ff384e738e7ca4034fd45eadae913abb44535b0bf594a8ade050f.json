{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '../apiService';\nexport default {\n  data() {\n    return {\n      recipe: {\n        title: '',\n        ingredients: '',\n        instructions: '',\n        image: null\n      }\n    };\n  },\n  created() {\n    this.fetchRecipe();\n  },\n  methods: {\n    fetchRecipe() {\n      const recipeId = this.$route.params.id;\n      api.get(`recipes/${recipeId}/`).then(response => {\n        this.recipe = response.data;\n      }).catch(error => {\n        console.error('Error fetching recipe:', error);\n      });\n    },\n    updateRecipe() {\n      const recipeId = this.$route.params.id;\n      let formData = new FormData();\n      formData.append('title', this.recipe.title);\n      formData.append('ingredients', this.recipe.ingredients);\n      formData.append('instructions', this.recipe.instructions);\n      formData.append('image', this.recipe.image);\n      api.put(`recipes/${recipeId}/`, formData, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        console.log('Recipe updated successfully:', response);\n        this.$router.push({\n          name: 'RecipeList'\n        }); // Navigate to RecipeList view\n      }).catch(error => {\n        console.error('Error updating recipe:', error);\n      });\n    },\n    handleFileUpload(event) {\n      this.recipe.image = event.target.files[0];\n    }\n  }\n};","map":{"version":3,"names":["api","data","recipe","title","ingredients","instructions","image","created","fetchRecipe","methods","recipeId","$route","params","id","get","then","response","catch","error","console","updateRecipe","formData","FormData","append","put","headers","log","$router","push","name","handleFileUpload","event","target","files"],"sources":["C:\\Users\\mantegbosh.assefa\\Desktop\\django\\frontend\\src\\components\\EditRecipe.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h2>Edit Recipe</h2>\r\n      <form @submit.prevent=\"updateRecipe\">\r\n        <div>\r\n          <label for=\"title\">Title</label>\r\n          <input type=\"text\" id=\"title\" v-model=\"recipe.title\" required>\r\n        </div>\r\n        <div>\r\n          <label for=\"ingredients\">Ingredients</label>\r\n          <textarea id=\"ingredients\" v-model=\"recipe.ingredients\" required></textarea>\r\n        </div>\r\n        <div>\r\n          <label for=\"instructions\">Instructions</label>\r\n          <textarea id=\"instructions\" v-model=\"recipe.instructions\" required></textarea>\r\n        </div>\r\n        <div>\r\n          <label for=\"image\">Image</label>\r\n          <input type=\"file\" id=\"image\" @change=\"handleFileUpload\" accept=\"image/*\">\r\n        </div>\r\n        <button type=\"submit\">Update Recipe</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import api from '../apiService';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        recipe: {\r\n          title: '',\r\n          ingredients: '',\r\n          instructions: '',\r\n          image: null\r\n        }\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchRecipe();\r\n    },\r\n    methods: {\r\n      fetchRecipe() {\r\n        const recipeId = this.$route.params.id;\r\n        api.get(`recipes/${recipeId}/`)\r\n          .then(response => {\r\n            this.recipe = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching recipe:', error);\r\n          });\r\n      },\r\n      updateRecipe() {\r\n        const recipeId = this.$route.params.id;\r\n        let formData = new FormData();\r\n        formData.append('title', this.recipe.title);\r\n        formData.append('ingredients', this.recipe.ingredients);\r\n        formData.append('instructions', this.recipe.instructions);\r\n        formData.append('image', this.recipe.image);\r\n  \r\n        api.put(`recipes/${recipeId}/`, formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        })\r\n          .then(response => {\r\n            console.log('Recipe updated successfully:', response);\r\n            this.$router.push({ name: 'RecipeList' }); // Navigate to RecipeList view\r\n          })\r\n          .catch(error => {\r\n            console.error('Error updating recipe:', error);\r\n          });\r\n      },\r\n      handleFileUpload(event) {\r\n        this.recipe.image = event.target.files[0];\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  /* Add custom styles for form and inputs as needed */\r\n  form {\r\n    max-width: 600px;\r\n    margin: auto;\r\n    padding: 1em;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  label {\r\n    font-weight: bold;\r\n    margin-bottom: 0.5em;\r\n  }\r\n  \r\n  input[type=\"text\"],\r\n  textarea {\r\n    width: 100%;\r\n    padding: 0.5em;\r\n    margin-bottom: 1em;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  input[type=\"file\"] {\r\n    margin-top: 0.5em;\r\n  }\r\n  \r\n  button[type=\"submit\"] {\r\n    padding: 0.5em 1em;\r\n    background-color: #007BFF;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button[type=\"submit\"]:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  \r\n  button[type=\"submit\"]:active {\r\n    background-color: #00408d;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA0BE,OAAOA,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZ,MAAME,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;MACtCb,GAAG,CAACc,GAAG,CAAC,WAAWJ,QAAQ,GAAG,EAC3BK,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACd,MAAK,GAAIc,QAAQ,CAACf,IAAI;